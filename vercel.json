{
  "redirects": [
    {
      "source": "/admin",
      "destination": "https://www.aggiesadmin.org/admin",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://aggies-attic-backend.fly.dev/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
